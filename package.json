{
  "name": "airtable-automation",
  "private": true,
  "scripts": {
    "make": "node out/script.js",
    "make:ts": "ts-node src/script.ts",
    "start": "concurrently \"npm:compile:watch\" \"npm:clasp:watch\" -c \"blue.bold,green.bold\" --kill-others",
    "compile": "tsc -p ./",
    "compile:watch": "tsc -p ./ -w",
    "clasp": "clasp push --force",
    "clasp:watch": "clasp push -w",
    "clasp:login": "clasp login",
    "clasp:status": "clasp login --status",
    "prebuild": "ts-node scripts/replace-env.ts",
    "build": "npm run compile && npm run clasp",
    "postbuild": "ts-node scripts/restore-env.ts",
    "lint": "bash scripts/lint.sh",
    "postinstall": "replace-in-file \"declare var console\" \"//declare var console\" node_modules/@types/google-apps-script/google-apps-script.base.d.ts"
  },
  "dependencies": {
    "airtable": "^0.10.1",
    "dotenv-safe": "^8.2.0",
    "googleapis": "^67.0.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.17",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/airtable": "^0.8.1",
    "@types/dotenv-safe": "^8.1.1",
    "@types/google-apps-script": "^1.0.21",
    "@types/node": "^14.14.21",
    "@types/node-fetch": "^2.5.8",
    "@types/nodemailer": "^6.4.0",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "clasp": "^1.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "markdownlint-cli": "^0.26.0",
    "prettier": "^2.2.1",
    "replace-in-file": "^6.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
